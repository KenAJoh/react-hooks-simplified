{
  "name": "react-snippets-simplified",
  "displayName": "React Hooks simplified",
  "description": "Why add many when few do trick?",
  "version": "0.3.0",
  "publisher": "kenajoh",
  "repository": "https://github.com/KenAJoh/react-snippets-simplified",
  "engines": {
    "vscode": "^1.60.0"
  },
  "keywords": [
    "snippets",
    "react",
    "javascript",
    "typescript",
    "hooks",
    "simple"
  ],
  "categories": [
    "Snippets"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets-js.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets-js.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets-ts.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets-ts.code-snippets"
      }
    ]
  }
}
