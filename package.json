{
  "name": "react-snippets-simplified",
  "displayName": "React snippets: Simplified",
  "description": "A collection of snippets without the bloat - Snippets for daily use",
  "version": "1.0.2",
  "publisher": "kenajoh",
  "repository": {
    "type": "git",
    "url": "https://github.com/KenAJoh/react-snippets-simplified"
  },
  "author": {
    "name": "Ken"
  },
  "homepage": "https://github.com/KenAJoh/react-snippets-simplified/blob/main/README.md",
  "engines": {
    "vscode": "^1.60.0"
  },
  "galleryBanner": {
    "color": "#121f2e",
    "theme": "dark"
  },
  "keywords": [
    "snippets",
    "react",
    "javascript",
    "typescript",
    "hooks",
    "simple"
  ],
  "categories": [
    "Snippets"
  ],
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "capabilities": {
    "virtualWorkspaces": true,
    "untrustedWorkspaces": {
      "supported": true
    }
  },
  "icon": "images/logo.png",
  "contributes": {
    "snippets": [
      {
        "language": "javascriptreact",
        "path": "./snippets/snippets-js.code-snippets"
      },
      {
        "language": "javascript",
        "path": "./snippets/snippets-js.code-snippets"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/snippets-ts.code-snippets"
      },
      {
        "language": "typescript",
        "path": "./snippets/snippets-ts.code-snippets"
      }
    ]
  },
  "license": "SEE LICENSE IN LICENSE"
}
